{
	"version": "2.0.0",
	"tasks": [
		{
			"command": "powershell.exe",
			"args": [
				"exec",
				"bundle install && bundle exec jekyll serve"
			],
			"type": "shell",
			"label": "Run Jekyll Server",
			"group": "test",
			"isBackground": true,
			"problemMatcher": [
				"$jekyll-error-watch"
			]
		}
	]
}